var form = "<%= j render 'form' %>";
var listId = "<%= @expense_list.id %>";
var current_panel = $('#expense_list_' + listId)
var createButton = $('.create-expense-list-button');

current_panel.after(form);
current_panel.remove();
createButton.hide();

form.on('click', '.back-link', function(){
  form.before(current_panel);
  form.remove();
  createButton.show();
});
